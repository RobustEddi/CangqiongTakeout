<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.SetmealMapper">
    <select id="getSetmealStatus" resultType="java.lang.Integer">
        select s.status from setmeal_dish sd left outer join setmeal s on sd.setmeal_id =s.id where sd.dish_id =#{id}


    </select>
</mapper>
